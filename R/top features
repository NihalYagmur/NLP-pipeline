require(tm) 
require(quanteda)
docs<- Corpus(DirSource(directory = "C:/Users/princess/Desktop/plain"))
docs <- tm_map(docs, removeNumbers)
docs <- tm_map(docs, removePunctuation)
docs <- tm_map(docs, content_transformer(tolower))
docs2 <-corpus(docs)
toks <- tokens(docs2)
in_dfm <- dfm(toks, remove = stopwords('en'))
topfeatures(in_dfm)
dtm <- TermDocumentMatrix(docs)
findAssocs(dtm, terms = "oven", corlimit = 0.3)
findAssocs(dtm, terms = "place", corlimit = 0.3)
